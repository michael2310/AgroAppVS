@model IEnumerable<AgroApp.Models.FarmModel>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FarmName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FarmStatus)
            </th>
            <th>
               <!--Html.DisplayNameFor(model => model.FarmOwner)-->-->
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FarmName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FarmStatus)
            </td>
            <td>
               <!--  Html.DisplayFor(modelItem => item.FarmOwner)-->
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FarmId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FarmId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FarmId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>